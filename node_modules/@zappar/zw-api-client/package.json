{
  "name": "@zappar/zw-api-client",
  "description": "ZapWorks API JS client",
  "version": "1.0.2",
  "license": "Proprietary",
  "main": "dist/node.js",
  "author": "Zappar Limited",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:bundle": "rollup -c",
