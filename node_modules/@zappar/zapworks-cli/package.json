{
  "name": "@zappar/zapworks-cli",
  "version": "2.0.5",
  "description": "A command-line utility to help you build and publish augmented reality experiences with ZapWorks",
  "main": "out/cli.js",
  "types": "out/*.d.ts",
  "bin": {
    "zapworks": "./bin/zapworks"
  },
  "scripts": {
    "test": "mocha -r ts-node/register src/tests/*.spec.ts",
    "cli:start": "node bin/zapworks",
    "cli:train": "node bin/zapworks train img.png --curved",
    "build": "node_modules/.bin/tsc",
    "watch": "node_modules/.bin/tsc -w",
    "build:zpp": "cd modules/zpp && npm run build",
    "cli:publish": "node_modules/.bin/tsc && node bin/zapworks publish",
    "cli:upload": "node_modules/.bin/tsc && node bin/zapworks upload",
    "cli:crush": "node_modules/.bin/tsc && node bin/zapworks zppclean",
    "cli:init": "node_modules/.bin/tsc && node bin/zapworks init",
    "cli:help": "node_modules/.bin/tsc && node bin/zapworks help",
    "cli:serve": "node_modules/.bin/tsc && node bin/zapworks serve"
  },
  "files": [
    "/bin",
    "/out",
    "/public",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "cli"
  ],
  "author": "Zappar Limited",
  "license": "Proprietary",
  "dependencies": {
    "@zappar/http-server": "^0.0.1",
    "@zappar/imagetraining": "3.0.0",
    "@zappar/zw-api-client": "^1.0.2",
    "archiver": "^5.2.0",
    "arg": "^5.0.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.9.0",
    "configstore": "^5.0.1",
    "debug": "^4.3.1",
    "del": "^6.0.0",
    "events": "^3.3.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "image-size": "^1.0.1",
    "inquirer": "^7.3.3",
    "is-directory": "^0.3.1",
    "jszip": "^3.7.1",
    "node-fetch": "^2.6.1",
    "portfinder": "^1.0.32",
    "promise-fs": "^2.1.1",
    "qrcode-terminal": "^0.12.0",
    "selfsigned": "^2.0.0",
    "tmp": "^0.2.1",
    "unused-filename": "^2.1.0",
    "update-notifier": "5.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/configstore": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/del": "^4.0.0",
    "@types/events": "^3.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.7",
    "@types/promise-fs": "^2.1.1",
    "@types/tmp": "^0.2.0",
    "@types/update-notifier": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
